<?xml version="1.0" encoding="utf-8"?><LinearLayout    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical"    >    <TextView        android:textColor="#000000"        android:padding="10dp"        android:id="@+id/tvSub"        android:textStyle="bold"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:text="SUB ITEM"/>    <View        android:layout_width="match_parent"        android:layout_height="1dp"        android:background="@color/colorPrimary"        android:id="@+id/vSub"/>    <androidx.recyclerview.widget.RecyclerView        android:id="@+id/rvItemInspeksi"        android:layout_width="match_parent"        android:layout_height="match_parent"/></LinearLayout>