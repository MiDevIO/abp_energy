<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    tools:context=".Login.SectionActivity">    <LinearLayout        android:id="@+id/linearLayout7"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal"        android:paddingLeft="5dp"        android:paddingRight="5dp"        android:weightSum="2"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <LinearLayout            android:layout_width="0dp"            android:layout_height="match_parent"            android:layout_weight="1.8"            android:orientation="horizontal">            <com.google.android.material.textview.MaterialTextView                android:id="@+id/tvIdSect"                style="@style/Widget.MaterialComponents.TextView"                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="0.5"                android:gravity="center_vertical|center_horizontal"                android:text="@string/devisi"                android:textColor="@color/colorPrimaryDark" />            <com.google.android.material.textview.MaterialTextView                android:id="@+id/tvSection"                style="@style/Widget.MaterialComponents.TextView"                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="1.5"                android:gravity="center_vertical|center_horizontal"                android:text="@string/devisi"                android:textColor="@color/colorPrimaryDark"                android:textStyle="bold" />        </LinearLayout>        <com.google.android.material.checkbox.MaterialCheckBox            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="0.2" />    </LinearLayout>    <View        android:layout_width="match_parent"        android:layout_height="1dp"        android:background="@color/colorPrimaryDark"        app:layout_constraintBottom_toBottomOf="parent" /></androidx.constraintlayout.widget.ConstraintLayout>