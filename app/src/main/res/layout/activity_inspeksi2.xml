<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".HSE.Inspeksi.Form.InspeksiActivity"    android:orientation="vertical">    <ScrollView        android:id="@+id/pullRefreshNewInspeksi"        android:layout_width="match_parent"        android:layout_height="wrap_content">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="vertical">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal">                <TextView                    android:paddingLeft="10dp"                    android:text="Tanggal"                    android:gravity="center_vertical"                    android:layout_width="0dp"                    android:layout_height="match_parent"                    android:layout_weight="0.5"/>                <com.google.android.material.textfield.TextInputLayout                    android:layout_width="0dp"                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"                    android:layout_height="match_parent"                    android:padding="5dp"                    android:id="@+id/tilTglInspeksi"                    android:hint="Tanggal Inspeksi"                    android:layout_weight="1.5">                    <com.google.android.material.textfield.TextInputEditText                        android:layout_width="match_parent"                        android:clickable="true"                        android:id="@+id/inTglInspeksi"                        android:focusable="false"                        android:inputType="none"                        android:layout_height="match_parent"/>                </com.google.android.material.textfield.TextInputLayout>            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal">                <TextView                    android:paddingLeft="10dp"                    android:text="Perusahaan"                    android:gravity="center_vertical"                    android:layout_width="0dp"                    android:layout_height="match_parent"                    android:layout_weight="0.5"/>                <com.google.android.material.textfield.TextInputLayout                    android:layout_width="0dp"                    android:id="@+id/tilInspeksiPerusahaan"                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"                    android:layout_height="match_parent"                    android:padding="5dp"                    android:hint="Perusahaan"                    android:layout_weight="1.5">                    <com.google.android.material.textfield.TextInputEditText                        android:layout_width="match_parent"                        android:id="@+id/inInspeksiPerusahaan"                        android:maxLines="1"                        android:focusable="false"                        android:imeOptions="actionNext"                        android:layout_height="match_parent"/>                </com.google.android.material.textfield.TextInputLayout>            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal">                <TextView                    android:paddingLeft="10dp"                    android:text="Lokasi"                    android:gravity="center_vertical"                    android:layout_width="0dp"                    android:layout_height="match_parent"                    android:layout_weight="0.5"/>                <com.google.android.material.textfield.TextInputLayout                    android:layout_width="0dp"                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"                    android:layout_height="match_parent"                    android:padding="5dp"                    android:id="@+id/tilInspeksiLokasi"                    android:hint="Lokasi Inspeksi"                    android:layout_weight="1.5">                    <com.google.android.material.textfield.TextInputEditText                        android:layout_width="match_parent"                        android:id="@+id/inInspeksiLokasi"                        android:layout_height="match_parent"                        android:imeOptions="actionNext"                        android:maxLines="1"                        />                </com.google.android.material.textfield.TextInputLayout>            </LinearLayout>        </LinearLayout>    </ScrollView>    <RelativeLayout        android:id="@+id/bottomLayout"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_alignParentBottom="true">        <com.google.android.material.button.MaterialButton            android:id="@+id/btnSkip"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            style="@style/Widget.MaterialComponents.Button.TextButton"            android:backgroundTint="@color/white"            android:layout_gravity="bottom|right"            android:text="Batal"            android:textColor="@color/colorPrimaryDark"            android:textAllCaps="false"            />        <LinearLayout            android:id="@+id/indicatorContainer"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerInParent="true"            android:orientation="horizontal">        </LinearLayout>        <com.google.android.material.button.MaterialButton            android:id="@+id/btnNext"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            style="@style/Widget.MaterialComponents.Button.TextButton"            android:backgroundTint="@color/white"            android:layout_gravity="bottom|right"            android:text="Berikutnya"            android:layout_alignParentEnd="true"            android:textColor="@color/colorPrimaryDark"            android:textAllCaps="false"            />    </RelativeLayout></RelativeLayout>