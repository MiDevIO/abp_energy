<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".Service.BarcodeScannerActivity">    <SurfaceView        android:id="@+id/cameraSrufaceView"        app:layout_constraintBottom_toTopOf="@id/viewDivider"        app:layout_constraintTop_toTopOf="parent"        android:layout_width="match_parent"        android:layout_height="0dp"/>    <View        android:background="@color/colorPrimary"        android:id="@+id/viewDivider"        android:layout_width="match_parent"        android:layout_height="4dp"        app:layout_constraintBottom_toTopOf="@id/textScanResult"/>    <TextView        android:id="@+id/textScanResult"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:gravity="center"        android:padding="20dp"        android:textColor="@color/colorAccent"        android:textSize="20sp"        android:textStyle="bold"        app:layout_constraintBottom_toBottomOf="parent"/></androidx.constraintlayout.widget.ConstraintLayout>