<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_margin="10dp"    app:cardElevation="30dp"    android:id="@+id/cvInfoList"    app:cardCornerRadius="20dp"    app:cardBackgroundColor="@color/white">    <LinearLayout        android:padding="10dp"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical">        <LinearLayout            android:padding="10dp"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="horizontal">            <TextView                android:layout_width="0dp"                android:layout_weight="0.5"                android:layout_height="wrap_content"                android:text="Judul"/>            <TextView                android:id="@+id/tvJudulBuletin"                android:layout_width="0dp"                android:layout_weight="1.5"                android:layout_height="wrap_content"                android:text="Judul"/>        </LinearLayout>        <LinearLayout            android:padding="10dp"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="horizontal">            <TextView                android:layout_width="0dp"                android:layout_weight="0.5"                android:layout_height="wrap_content"                android:text="Pesan"/>            <TextView                android:id="@+id/tvPesanBuletin"                android:layout_width="0dp"                android:layout_weight="1.5"                android:layout_height="wrap_content"                android:text="Judul"/>        </LinearLayout>        <LinearLayout            android:padding="10dp"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="horizontal">            <TextView                android:layout_width="0dp"                android:layout_weight="0.5"                android:layout_height="wrap_content"                android:text="Tanggal"/>            <TextView                android:id="@+id/tvTglBuletin"                android:layout_width="0dp"                android:layout_weight="1.5"                android:layout_height="wrap_content"                android:text="Judul"/>        </LinearLayout>    </LinearLayout></androidx.cardview.widget.CardView>