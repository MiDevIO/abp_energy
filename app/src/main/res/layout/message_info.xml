<?xml version="1.0" encoding="utf-8"?><com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="wrap_content"    android:layout_height="wrap_content"    style="@style/Widget.MaterialComponents.CardView"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_margin="20dp"    app:cardCornerRadius="20dp"    app:cardElevation="50dp"    app:cardBackgroundColor="@color/white"    android:outlineAmbientShadowColor="@color/white"    android:outlineSpotShadowColor="@color/white"    android:id="@+id/cvInfo"    >    <LinearLayout        android:layout_margin="20dp"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:orientation="vertical">        <TextView            android:textColor="#000000"            android:textStyle="bold"            android:id="@+id/tvJudul"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:text="Omicron"/>        <View            android:layout_width="match_parent"            android:layout_height="5dp"/>        <TextView            android:textColor="#181D22"            android:paddingLeft="10dp"            android:id="@+id/tvPesan"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:text="Berbahaya"/>        <View            android:layout_width="match_parent"            android:layout_height="10dp"/>        <TextView            android:textColor="#1755B3"            android:textAlignment="viewEnd"            android:paddingEnd="10dp"            android:paddingLeft="30dp"            android:id="@+id/tvTgl"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:textSize="10sp"            android:text="1 Maret 2022"/>    </LinearLayout></com.google.android.material.card.MaterialCardView>