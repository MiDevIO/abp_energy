<?xml version="1.0" encoding="utf-8"?><androidx.coordinatorlayout.widget.CoordinatorLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".HomeActivity">    <com.google.android.material.bottomappbar.BottomAppBar        android:layout_width="match_parent"        style="@style/Widget.MaterialComponents.BottomAppBar"        android:layout_height="wrap_content"        android:id="@+id/bottomAppBar"        app:fabAlignmentMode="end"        app:fabCradleRoundedCornerRadius="50dp"        app:fabAnimationMode="slide"        app:backgroundTint="@color/bgApprove"        android:layout_gravity="bottom"/>    <com.google.android.material.floatingactionbutton.FloatingActionButton        android:layout_width="wrap_content"        android:layout_height="wrap_content"        style="@style/Widget.MaterialComponents.FloatingActionButton"        android:id="@+id/fabIC"        app:layout_anchor="@id/bottomAppBar"        android:src="@drawable/ic_add_black_24dp"        app:tint="@color/white"        android:backgroundTint="@color/colorPrimaryDark"/></androidx.coordinatorlayout.widget.CoordinatorLayout>